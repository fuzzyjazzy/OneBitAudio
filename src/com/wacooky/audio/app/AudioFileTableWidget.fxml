<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>

<fx:root type="HBox" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1">
	<children>
		<TableView fx:id="fileInfoTable" onDragDropped="#pathDropped" onDragOver="#pathDragOver" onKeyPressed="#keyPressed" onKeyReleased="#keyReleased" prefHeight="316.0" prefWidth="620.0">
			<columns>
				<TableColumn fx:id="nameColumn" minWidth="100.0" prefWidth="200.0" text="name" />
				<TableColumn fx:id="sfColumn" minWidth="100.0" prefWidth="100.0" text="sf" />
				<TableColumn fx:id="chColumn" minWidth="30.0" prefWidth="30.0" text="ch" />
				<TableColumn fx:id="timeColumn" minWidth="100.0" prefWidth="100.0" text="time" />
				<TableColumn fx:id="typeColumn" prefWidth="75.0" text="type" />
			</columns>
		</TableView>
	</children>
</fx:root>
